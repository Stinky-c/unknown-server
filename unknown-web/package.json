{
  "name": "unknown-web",
  "private": true,
  "version": "0.0.0",
  "source": [
    "src/*.html",
    "src/**/*.html"
  ],
  "scripts": {
    "dev": "parcel",
    "build": "parcel build --reporter @parcel/reporter-bundle-buddy"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.16.1",
    "@parcel/plugin": "^2.16.1",
    "@parcel/reporter-bundle-buddy": "^2.16.1",
    "@tailwindcss/postcss": "^4.1.17",
    "daisyui": "^5.4.7",
    "parcel": "^2.14.0",
    "parcel-namer-rewrite": "2.10.3-rc.5",
    "postcss": "^8.5.6",
    "posthtml": "^0.16.7",
    "posthtml-doctype": "^1.1.1",
    "posthtml-include": "^2.0.1",
    "tailwindcss": "^4.1.17"
  },
  "dependencies": {
    "htmx.org": "^2.0.8",
    "zxcvbn": "^4.4.2"
  },
  "targets": {
    "default": {
      "context": "browser",
      "includeNodeModules": [
        "htmx.org",
        "zxcvbn"
      ]
    }
  },
  "parcel-namer-rewrite": {
    "rules": {
      "(.*).css": "assets/css/$1{.hash}.css",
      "(.*).js": "assets/js/$1{.hash}.js",
      "(.*).png": "assets/img'$1{.hash}.png"
    }
  }
}